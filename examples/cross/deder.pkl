amends "../../config/DederProject.pkl"

local const scalaVersions = List("2.13.18", "3.7.4")
local const scalaJsVersion = "1.20.2"

local const commonModules =
  scalaVersions
    .map((sv) ->
      new CrossJsModules {
        _root = "common"
        _scalaVersion = sv
        _scalaJSVersion = scalaJsVersion
      }
        .get
        .all
    )
    .flatten()

modules {
  ...commonModules
}
